# Environment variables for Revit Key Backend

# Database
DATABASE_URL=sqlite:///./revit_keys.db

# JWT
JWT_SECRET_KEY=your_super_secret_jwt_key_change_this
ACCESS_TOKEN_EXPIRE_MINUTES=60

# CORS
CORS_ORIGINS=*

# SMTP Email (for OTP password reset)
# Leave empty for dev mode (OTP printed to console)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=
SMTP_PASSWORD=
SMTP_FROM_EMAIL=noreply@example.com

# OTP settings
OTP_EXPIRE_MINUTES=10
